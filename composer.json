    "keywords": ["json", "nested json", "Flatten nested json", "json flattener", "convert json to csv", "csv", "csv writer"],
    "license": "MIT",
    "authors": [
        {
            "name": "tonirilix",
            "email": "jesusmontes.mx@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "NestedJsonFlattener\\": "src/"
        }
    },    
    "scripts": {        
        "test": "./vendor/bin/phpunit --configuration ./phpunit.xml.dist"
    },
    "require": {
        "php": "^7.1 || ^8.0",
        "peekmo/jsonpath": "dev-master"
    },
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.5 || ^9.6"
    }
